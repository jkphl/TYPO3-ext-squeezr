{
  "name": "jkphl/squeezr",
  "type": "typo3-cms-extension",
  "description": "Easily integrate squeezr (https://github.com/jkphl/squeezr) into your TYPO3 setup",
  "homepage": "https://github.com/jkphl/TYPO3-ext-squeezr",
  "license": "GPL-2.0+",
  "require": {
    "typo3/cms": ">=6.0"
  },
  "autoload": {
    "psr-4": {
      "Tollwerk\\Squeezr\\": "Classes/"
    }
  },
  "replace": {
    "squeezr": "self.version",
    "typo3-ter/squeezr": "self.version"
  },
  "authors": [
    {
      "name": "Joschi Kuphal",
      "email": "joschi@tollwerk.de"
    }
  ],
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
