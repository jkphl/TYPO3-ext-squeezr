{
  "name": "tollwerk/tw-squeezr",
  "type": "typo3-cms-extension",
  "description": "TYPO3 extension version of squeezr, another take on device-aware adaptive images and server side CSS3 media queries",
  "homepage": "https://github.com/tollwerk/TYPO3-ext-squeezr",
  "license": "GPL-2.0+",
  "require": {
    "typo3/cms": ">=6.0"
  },
  "autoload": {
    "psr-4": {
      "Tollwerk\\Squeezr\\": "Classes/"
    }
  },
  "replace": {
    "tw_squeezr": "self.version",
    "typo3-ter/tw_squeezr": "self.version"
  },
  "authors": [
    {
      "name": "Joschi Kuphal",
      "email": "joschi@tollwerk.de"
    }
  ],
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
